{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**", "!dist/tmp/**"]
        },
        "release": {
            "dependsOn": ["^release"],
            "outputs": ["dist/**", "!dist/tmp/**"]
        }
    }
}
